   "source": [
    "#Task 2:\n",
    "#Modify your program from the previous exercise to read the book you downloaded and process the rest of the words as before.\n",
    "#A. Then modify the program to count the total number of words in the book, and the number of times each word is used.\n",
    "#B. print the 20 most frequently-used words in the book.\n",
    "#C. Print the number of different words used in the book\n",
    "#Optional: write a function that takes in any number of books and produce the book with the greatest number of words\n",
    "\n",
    "import string\n",
    "\n",
    "specialCharacters = string.punctuation\n",
    "fin = open('mybook.txt', encoding=\"utf-8\")\n",
    "dict = {}\n",
    "for line in fin:\n",
    "    for word in line.split():\n",
    "        word = word.strip(specialCharacters)\n",
    "        word = word.lower()\n",
    "        if word not in dict: # condition for adding the words list into dictionary.\n",
    "            dict[word] = 1\n",
    "        else:\n",
    "            dict[word] = dict[word] + 1\n",
    "print('total number of words') \n",
    "print(str(sum(dict.values()))) # printing the count of words by adding the values of words.\n",
    "print('number of times each word is used')\n",
    "print(dict) \n",
    "sortedValues = sorted(dict, key=dict.get, reverse=True)[:20] # condition for sorting the 20 most frequent words used \n",
    "print('20 most frequent words used in the book')\n",
    "for i in sortedValues:\n",
    "    print(i, dict[i])\n",
    "print('number of different words in book')\n",
    "print(len(dict))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 15,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Slope value is:\n",
      "1.129891498197954\n"
     ]
    }
   ],
   "source": [
    "# Task -3\n",
    "\n",
    "import string\n",
    "import numpy # for importing slope,intercept functions\n",
    "import math\n",
    "\n",
    "specialCharacters = string.punctuation\n",
    "fin = open('mybook.txt', encoding=\"utf-8\")\n",
    "dict = {}\n",
    "for line in fin:\n",
    "    for word in line.split():\n",
    "        word = word.strip(specialCharacters)\n",
    "        word = word.lower()\n",
    "        if word not in dict:\n",
    "            dict[word] = 1\n",
    "        else:\n",
    "            dict[word] = dict[word] + 1\n",
    "sortedValues = sorted(dict, key=dict.get, reverse=True)\n",
    "\n",
    "nextdict = {}\n",
    "rank = 1\n",
    "frequencylist = []\n",
    "ranklist = []\n",
    "for i in sortedValues:\n",
    "    frequencylist.append(math.log10(dict[i])) #creating list of frequencies \n",
    "    ranklist.append(math.log10(rank)) #creating list of ranks\n",
    "    rank = rank + 1\n",
    "\n",
    "slope, intercept = numpy.polyfit(frequencylist, ranklist, 1) # estimating slope value form the log values of frequency and ranks\n",
    "slope = -1 * slope\n",
    "print('slope value is:')\n",
    "print(slope)"
   ]
